    target = 'query',
    source = [
        "multi_plan_runner.cpp",
        "new_find.cpp",
        "plan_ranker.cpp",
    ],
    LIBDEPS = [
        "$BUILD_DIR/mongo/db/exec/exec"
    ],
)

